{
	"name" : "satelines/auto-form-bundle",
	"type" : "symfony-bundle",
	"minimum-stability" : "dev",
	"prefer-stable" : true,
	"description" : "Automatize form creation",
	"keywords" : [
		"symfony",
		"form",
		"field",
		"automatize",
		"automatization",
		"magic"
	],
	"homepage" : "https://github.com/satelines/AutoFormBundle",
	"license" : "MIT",
	"authors" : [{
			"name" : "David ALLIX",
			"homepage" : "http://a2lix.fr"
		}
	],
	"require" : {
		"php" : "^7.1.3",
		"symfony/form" : "^4.0",
		"symfony/doctrine-bridge" : "^4.0"
	},
	"require-dev" : {
		"symfony/validator" : "^4.0",
		"phpunit/phpunit" : "~4.1",
		"friendsofphp/php-cs-fixer" : "~1.10",
		"doctrine/orm" : "^2.6"
	},
	"suggest" : {
		"a2lix/translation-form-bundle" : "For translation form"
	},
	"autoload" : {
		"psr-4" : {
			"A2lix\\AutoFormBundle\\" : ""
		},
		"exclude-from-classmap" : [
			"/Tests/"
		]
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "0.x-dev"
		}
	}
}